{
  "name": "template-astro",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:astro": "astro dev",
    "dev:optimizeImage": "node ./src/integrations/optimizeImage.js",
    "build": "npm-run-all -s build:*",
    "build:astro": "astro build",
    "build:relativePath": "node ./src/integrations/convertRelativePath.js",
    "build:format": "node ./src/integrations/beautifyHTML.js",
    "preview": "astro preview",
    "lint": "npm-run-all -s lint:*",
    "lint:html": "markuplint \"htdocs/**/*.html\"",
    "lint:css": "stylelint \"htdocs/**/*.css\"",
    "lint:js": "eslint \"src/**/*.js\"",
    "format": "html-beautify \"htdocs/**/*.html\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-futenma/template-astro"
  },
  "dependencies": {
    "astro": "^5.1.2",
    "lil-gui": "^0.20.0",
    "three": "^0.171.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@markuplint/astro-parser": "^4.6.5",
    "@typescript-eslint/parser": "^7.16.0",
    "chokidar": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^1.2.2",
    "fs-extra": "^11.2.0",
    "image-size": "^1.1.1",
    "js-beautify": "^1.15.1",
    "markuplint": "^4.9.2",
    "npm-run-all": "^4.1.5",
    "sharp": "^0.33.4",
    "stylelint": "^16.8.1",
    "stylus": "^0.64.0",
    "typescript": "^5.5.3",
    "vite-plugin-glsl": "^1.3.1"
  }
}
